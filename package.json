{
  "name": "rhoknov-blog",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm:dev:content\" \"npm:dev:svelte\"",
    "dev:content": "contentlayer dev",
    "dev:svelte": "vite dev",
    "build": "svelte-kit sync && contentlayer build && vite build",
    "build:content": "contentlayer build",
    "build:content-clear": "contentlayer build --clearCache",
    "build:svelte": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/adapter-cloudflare": "^4.7.3",
    "@sveltejs/adapter-node": "^5.2.9",
    "@sveltejs/adapter-static": "^3.0.6",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/eslint": "^9.6.0",
    "@types/three": "^0.159.0",
    "contentlayer": "^0.3.4",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.36.0",
    "globals": "^15.0.0",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "sass-embedded": "^1.83.4",
    "svelte": "^4.2.7",
    "svelte-check": "^4.0.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.0.0",
    "vite": "^5.4.11"
  },
  "type": "module",
  "dependencies": {
    "@threlte/core": "^7.3.1",
    "@threlte/extras": "^8.11.5",
    "concurrently": "^9.1.0",
    "contentlayer": "^0.3.4",
    "github-markdown-css": "^5.8.1",
    "mdsvex": "^0.12.3",
    "parse-md": "^3.0.3",
    "scss": "^0.2.4",
    "svelte-markdown": "^0.4.1",
    "three": "^0.159.0",
    "zenn-content-css": "^0.1.157"
  }
}
